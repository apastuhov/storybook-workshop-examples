    "babel-preset-vue": "^2.0.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "5.0.0",
    "@types/storybook__addon-centered": "3.4.0",
    "@types/storybook__addon-links": "3.3.4",
